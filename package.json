{
  "name": "angular-with-tailwind",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-markdown": "cd remark;node example.js < example.md > example.html ",
    "compile-plugin": "tsc --skipLibCheck --module umd",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node local.js",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-with-tailwind:server:production --bundleDependencies all",
    "server": "node local.js",
    "build:prod": "npm run build:ssr",
    "serve:prerender": "node static.js",
    "build:prerender": "npm run build:prod && node dist/prerender.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ng-toolkit/universal": "^8.0.3",
    "@nguniversal/express-engine": "v8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "angular-code-builder": "0.0.5",
    "angular-kawaii": "^1.0.0",
    "express": "^4.15.2",
    "install": "^0.13.0",
    "ngx-build-plus": "^8.1.3",
    "npm": "^6.13.6",
    "prismjs": "^1.27.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.2",
    "@nguniversal/common": "8.1.0",
    "domino": "^2.1.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.0",
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "detab": "^2.0.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "rehype-stringify": "^6.0.0",
    "remark": "^11.0.1",
    "remark-html": "^10.0.0",
    "remark-parse": "^7.0.1",
    "remark-rehype": "^5.0.0",
    "tailwindcss": "^1.1.1",
    "trim-lines": "^1.1.2",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "unified": "^8.3.2",
    "unified-stream": "^1.0.5",
    "unist-builder": "^1.0.4",
    "webpack-cli": "^3.3.5"
  }
}
